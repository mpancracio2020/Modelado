<?xml version="1.0"?>
<robot name="myrobot">
    <material name="blue">
        <color rgba="0 0 0.8 1" />
    </material>
    

    <link name="base_link">

        <inertial>
            <origin xyz="0 0 0.1" />
            <mass value="10000.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="1.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin xyz="0 0 0.5" />
            <geometry>
                <box size="4 4 1.5" />
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin xyz="0 0 0.5" />
            <geometry>
             <box size="4 4 1.5" />
            </geometry>
        </collision>

    </link>

    
    <link name="left">

        <inertial>
            <origin rpy="0 0 0" xyz="0 -2.5 1.3" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="0.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin rpy="0 0 0" xyz="0 -2.5 1.3" />
            <geometry>
                <box size="4 2.1 0.1" />
            </geometry>
            <material name="blue"/>
            
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 -2.5 1.3" />
            <geometry>
             <box size="4 2.1 0.1" />
            </geometry>
        </collision>

    </link>
    <link name="right">

        <inertial>
            <origin rpy="0 0 0" xyz="0 2.5 1.4" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="0.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin rpy="0 0 0" xyz="0 2.5 1.4" />
            <geometry>
                <box size="4 2.1 0.1" />
            </geometry>
            <material name="blue"/>
            
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 2.5 1.4" />
            <geometry>
             <box size="4 2.1 0.1" />
            </geometry>
        </collision>

    </link>

    <joint name="base_to_body" type="fixed">
        <parent link="base_link" />
        <child link="left" />
        <origin xyz="0 0 1.3" rpy="0.785 0 0" />
        
    </joint>
    <joint name="base_to_body_2" type="fixed">
        <parent link="base_link" />
        <child link="right" />
        <origin xyz="0 0 1.3" rpy="-0.785 0 0" />
        
    </joint>
    

</robot>
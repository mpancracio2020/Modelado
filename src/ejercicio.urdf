<?xml version="1.0"?>
<robot name="myrobot">

    <link name="base_link">

        <inertial>
            <origin xyz="0 0 0.1" />
            <mass value="10000.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="1.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin xyz="0 0 0.1" />
            <geometry>
                <box size="0.4 0.4 0.2" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.1" />
            <geometry>
             <box size="0.4 0.4 0.2" />
            </geometry>
        </collision>

    </link>

    <material name="blue">
        <color rgba="0 0 0.8 1" />
    </material>

    <link name="body_link">

        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.5" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="0.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin rpy="0 0 0" xyz="0 0 0.5" />
            <geometry>
                <box size="0.1 0.1 0.8" />
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.5" />
            <geometry>
             <box size="0.1 0.1 0.8" />
            </geometry>
        </collision>

    </link>

     <link name="end_link">

        <inertial>
            <origin rpy="0 0 0" xyz="-0.2 0 0.8" />
            <mass value="1.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
            iyy="0.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin rpy="0 0 0" xyz="-0.2 0 0.8" />
            <geometry>
                <box size="0.4 0.1 0.1" />
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.8" />
            <geometry>
             <box size="0.4 0.1 0.1" />
            </geometry>
        </collision>

    </link>

    <joint name="base_to_body" type="fixed">
        <parent link="base_link" />
        <child link="body_link" />
        <origin xyz="0.05 0 0.1" />
        <axis xyz="1 0 0" />
    </joint>

     <joint name="body_to_end" type="continuous">
        <parent link="body_link" />
        <child link="end_link" />
        
        <origin xyz="0.05 0 0.1" />
        <axis xyz="0 0 1" />

        
    </joint>
    

</robot>